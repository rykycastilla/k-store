{
  "expo": {
    "name": "K-Store",
    "slug": "K-Store",
    "version": "1.0.0",
    "scheme": "kstore",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rykyce.kstore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1c27ce2c-24ef-4d48-b26a-07ec7e6f58a5"
      }
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ]
  }
}
